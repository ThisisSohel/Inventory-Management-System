@model IMS.Entity.Entities.Supplier
@{
    ViewBag.Title = "Details";
}

<h2>Details information about the customer</h2>

<div>
    <h4>Supplier ID: @Model.Id</h4>
    <h4>Supplier Name: @Model.SupplierName</h4>
    <h4>Supplier Number: @Model.SupplierNumber</h4>
    <h4>Supplier EmailAddress: @Model.EmailAddress</h4>
    <h4>Supplier's Address': @Model.SupplierAddress</h4>
    <h4>Supplier is Created by: @Model.CreatedBy</h4>
    <h4>Supplier is Created at: @Model.CreatedDate</h4>
    <h4>Supplier is Modyfied By: @Model.ModifyBy</h4>
    <h4>Supplier is Modyfied By: @Model.ModifyDate</h4>
</div>

<p>
    @Html.ActionLink("Update", "Update", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
